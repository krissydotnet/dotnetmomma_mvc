@model IList<DotNetMommaShared.Models.Tag>

<div class="tags blog-sidebar">
    <h3>Tags</h3>

    @if (Model.Count > 0)
    {
        foreach (var tag in Model)
        {
    <span class="tags-div">
        @Html.TagLink(tag)
    </span>
           
        }
    }
    else
    {
        <p>No tags found!</p>
    }
</div>
